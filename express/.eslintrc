{
    "rules": {
        "indent": [ 2, "tab", {"SwitchCase": 1} ],
        "quotes": [ 1, "single" ],
        "linebreak-style": [ 2, "unix" ],
        "semi": [ 2, "always" ],
        "no-console": 1,
        "no-unused-vars": 1
    },
    "env": {
        "node": true
    },
    "extends": "eslint:recommended"
}
