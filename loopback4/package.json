{
  "name": "loopback4",
  "version": "0.0.1",
  "description": "loopback4-test-bench",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t loopback4 .",
    "docker:run": "docker run -p 3000:3000 -d loopback4",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "jcolekaplan <jacob.kaplan@contrastsecurity.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@contrast/test-bench-utils": "file://../test-bench-utils",
    "@loopback/boot": "^3.0.2",
    "@loopback/booter-lb3app": "^2.2.12",
    "@loopback/core": "^2.11.0",
    "@loopback/repository": "^3.1.0",
    "@loopback/rest": "^8.0.0",
    "@loopback/rest-explorer": "^3.0.2",
    "@loopback/service-proxy": "^3.0.2",
    "body-parser": "^1.19.0",
    "compression": "^1.0.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.5.2",
    "express-ejs-layouts": "^2.5.0",
    "helmet": "^3.10.0",
    "lodash": "^4.17.19",
    "loopback": "^3.22.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^6.2.0",
    "loopback-component-storage": "^3.6.3",
    "serve-favicon": "^2.0.1",
    "tslib": "^2.0.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.5",
    "source-map-support": "^0.5.19",
    "@loopback/testlab": "^3.2.7",
    "@types/node": "^10.17.37",
    "@loopback/eslint-config": "^10.0.1",
    "typescript": "~4.0.3",
    "@contrast/eslint-config": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "loopback-cli": "^5.0.3",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1"
  }
}
