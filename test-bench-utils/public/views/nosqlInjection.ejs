<h1 class="page-header"><%= name %></h1>
<% include ../partials/ruleInfo.ejs %>
<% sinkData.forEach(function(sink) { %>
<div class="row">
  <div class="col-xs-12 col-sm-6" style="padding-bottom: 30px;">
    <h4 class="sub-header">
      <code><%= sink.name %></code>
    </h4>
    <% if(descriptions[sink.name]) { %>
    <h5 class="sub-header">
      <strong><%= descriptions[sink.name] %></strong>
    </h5>
    <% } %>
    <form method="<%= sink.method %>" action="<%= sink.url %>/unsafe" target="_blank">
      <div class="form-group">
        <label>Input</label>
        <input name="input" class="form-control" value="<%= attackValues[sink.name] || 'attack!' %>" />
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
  </div>
</div>
<% }); %>
<% include ../partials/safeButtons %>
